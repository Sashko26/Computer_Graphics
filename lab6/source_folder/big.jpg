        <script>
        $('body').on('click', '.download-button',  function() {
            var element = $(this);
            var wallpaper_id = element.attr("data-id");
            var type = element.attr("data-type");
            var server = element.attr("data-server");
            var user_id = element.attr("data-user-id");

            $.post("get_download_link.php", {wallpaper_id: wallpaper_id, type: type, server: server, user_id: user_id},
                function(data){
                    if(data !== "") {
                        element.attr("disabled", "disabled");

                        window.location.href = data;

                        setTimeout(function(){
                            element.removeAttr("disabled");
                        },5000);
                    }
                });
        });

        var nextPage = $("#next_page");
        var prevPage = $("#prev_page");
        prevUrl = prevPage.attr("href");
        nextUrl = nextPage.attr("href");

        document.onkeydown = function(evt) {
            evt = evt || window.event;
            var j = evt.keyCode;
            var x = evt.target||evt.srcElement;
            if(x != "[object HTMLInputElement]") {
                if (j == 39 && undefined != nextUrl) {
                    window.location = nextUrl;
                }
                else if(j == 37 && undefined != prevUrl) {
                    window.location = prevUrl;
                }
            }
        }

        function rateWallpaper(wallpaper_id, source)
        {
            $.post("rating.php", {wallpaper_id: wallpaper_id, source: source},
                function(data){
                    $("#rate_" + wallpaper_id).html(data);
                });
        }
    </script>

    
